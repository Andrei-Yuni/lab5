@model BookingAppStore.Models.BookModel

<h2>Новая книга</h2>

@using (Html.BeginForm("Create", "Home", FormMethod.Post, new {enctype="multipart/form-data"}))
{
     @Html.LabelFor(model => model.Name, "Название предмета")
     <br />
     @Html.EditorFor(model => model.Name)
     <br />
     <br />
     @Html.LabelFor(model => model.NumberLab, "Номер лаб. раб.")
     <br />
     @Html.EditorFor(model => model.NumberLab)
     <br />
     <br />
     @Html.LabelFor(model => model.Variant, "Вариант")
     <br />
     @Html.EditorFor(model => model.Variant)
     <br />
     <br />
     <input type="file" id="fileToUpload" name="File" required />
     <br />
     <br />
     <input type="submit" value="Добавить" />
}
